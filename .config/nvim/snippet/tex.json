{
  "sect": {
		"prefix": ["section"],
		"body": [
			"\\\\section{$0}"
		],
		"description": "Insert section"
	},
  "equ": {
		"prefix": ["equ"],
		"body": [
			"\\\\[ $0 \\\\]"
		],
		"description": "Insert equation"
	},
  "sum": {
		"prefix": ["sum"],
		"body": [
			"\\\\sum_${1:from}^${2:until} $0"
		],
		"description": "Insert sum (sigma)"
	},
  "dq": {
		"prefix": ["dq"],
		"body": [
			"``$0''"
		],
		"description": "Insert sum (sigma)"
	}
}
