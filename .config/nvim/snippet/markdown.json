{
  "code": {
    "prefix": ["code"],
    "body": [
      "```${1:lang}",
      "```"
    ],
    "description": "Insert code block."
  },
  "italic": {
    "prefix": ["italic"],
    "body": [
      "*${1:text}*"
    ],
    "description": "Italic wrap"
  },
  "underline": {
    "prefix": ["underline"],
    "body": [
      "**${1:text}**"
    ],
    "description": "Underline wrap"
  },
  "combineitauline": {
    "prefix": ["combine"],
    "body": [
      "***${1:text}***"
    ],
    "description": "Italic and Underline wrap"
  },
  "title": {
    "prefix": ["title"],
    "body": [
      "---",
      "title: ${1:title}",
      "author: Mochammad Hanif R. (etrnal70)",
      "date: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
      "---"
    ],
    "description": "Standard YAML-format header."
  }
}
